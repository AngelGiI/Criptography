<Font foreground="[51,102,255]" encoding="UTF-8">#Copiamos la pr\303\241ctica 1.4 </Font>
mcdfpx:=proc(a::polynom,b::polynom,x,p::integer)
local dividendo,divisor,resto,resultado;
dividendo:=a mod p;
sort(dividendo);
divisor:=b mod p;
sort(divisor);
while degree(divisor,x)&gt;=0 do
resto:=rem(dividendo,divisor,x) mod p;
dividendo,divisor:=divisor,resto;
od;
resultado:=factor((dividendo*inversofp(lcoeff(dividendo),p)) mod p);
end:

inversofp:=proc(a::integer,p::integer)
local b,n,i,resultado;
if gcd(p,a)&lt;&gt;1 then 
print(&quot;No hay inverso&quot;);
else
b:=a mod p;
i:=b;
for n from 1 to p do
if i&lt;&gt;1 then
i:=b*n mod p;
resultado:=n;
elif b=1 then
resultado:=1;
fi;
od;
return resultado;
fi;
end:

<Font foreground="[51,102,255]">#Test de irreducibilidad en Fp[x]</Font>
testirreducibilidadfpx:=proc(f::polynom,x,p::prime)
local i, m, d, r, esirreducible; 
m:=x; 
d:=degree(f); 
esirreducible:=true;
i:=1;
while i&lt;floor(d/2) and esirreducible do 
m:=rem(m^p, f, x) mod p; 
r:=mcdfpx(f, m-x, x, p);
if r &lt;&gt; 1 then 
esirreducible:=false;
fi;
i:=i+1;
od; 
return esirreducible;
end:</Text-field>
</Input>
</Group>
