<Section collapsed="true" isCollapsible="true" drawButton="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title><Text-field style="Heading 2" layout="Heading 2">1.3. R=Z[i]</Text-field></Title>
<Group labelreference="L23" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">restart;
<Font foreground="[51,102,255]">#Algoritmo de euclides</Font>
decide:=proc(z)
local r,i,a,b,x,y,w;
r:=Re(z);
i:=Im(z);
a:=floor(r);
b:=floor(i);
x:=r-a;
y:=i-b;
if 1 &lt;= x+y and x &lt; y then
w:=a+I*(b+1)
elif 1 &lt;= x+y and y &lt;= x then
w:=a+1+I*b
else
w:=a+I*b
fi;
return w
end:

mcdzi:=proc(z1,z2)
local t,r;
t:=z1;
r:=z2;
while r &lt;&gt; 0 do
t,r:=r,t-r*decide(t/r)
od;
return t
end:</Text-field>
</Input>
</Group>
