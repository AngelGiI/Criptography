<Section collapsed="true" isCollapsible="true" drawButton="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title><Text-field style="Heading 1" layout="Heading 1">2. Algoritmo para calcular el teorema chino del resto (i.e. calcular el inverso).</Text-field></Title>
<Group labelreference="L26" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">restart;<Font foreground="[51,102,255]">
<Font encoding="UTF-8">#Copiamos parte de la pr\303\241ctica 1.1. con alguna modificaci\303\263n.</Font></Font>
euclidesextz:=proc(a::integer,b::integer) 
local mcd,resto,coefa,coefb,t,s,cociente,resultado; 
mcd,resto:=a,b; 
coefb,coefa:=0,1; 
t,s:=1,0; 
while resto&lt;&gt;0 do 
mcd,resto,cociente:=resto,irem(mcd,resto),iquo(mcd,resto); 
coefa,s:=s,coefa-cociente*s; 
coefb,t:=t,coefb-cociente*t; 
od;
return([abs(mcd), coefa, coefb])
end:

chino2:=proc(m::list,n::list) 
local coef,res; 
coef:=euclidesextz(n[1],n[2]); 
res:=coef[2]*m[2]*n[1]+coef[3]*m[1]*n[2]; 
return res;
end:

multelemlista:=proc(a::list) 
local i,p; 
p:=1; 
for i to numelems(a) do 
p:=p*a[i];
od; 
return p; 
end:

<Font foreground="[51,102,255]">#Algoritmo principal</Font>
chinogeneral:=proc(m::list,n::list)
local i,a,b;
a,b:=[],[];
for i from 1 to numelems(m)-1 do
a,b:=[op(a),m[i]],[op(a),n[i]];
od;
a,b:=multelemlista(a),multelemlista(b);
return chino2([a,m[numelems(m)]],[b,n[numelems(m)]])
end:</Text-field>
</Input>
</Group>
