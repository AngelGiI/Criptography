<Font foreground="[51,102,255]">#Creamos un algoritmo que nos pase los polinomios a vector (aux)
</Font>PasoPolinomioAVector:=proc(p::integer,pol::polynom) 
local f,g,L; 
L:=[]; 
f:=`mod`(expand(pol),p); 
while f &lt;&gt; 0 do 
L:=[op(L),[lcoeff(f,x),degree(f,x)]]; 
f:=expand(f-lcoeff(f,x)*x^degree(f,x)) 
od; 
L; 
end:

<Font foreground="[51,102,255]">#Creamos un polinomio g a partir de uno dado y una lista (aux)</Font>
CreaElPolinomiog:=proc(p::integer,f::polynom,L::list) 
local g,a,b,i; 
g:=0; 
for i to nops(L) do 
a:=L[i][1]; 
b:=L[i][2]/p; 
g:=g+a*x^b 
od; 
g;
end:

<Font foreground="[51,102,255]">#Algoritmo principal</Font>
LibreCuadrados:=proc(polmonico::polynom,p::integer,x) 
local f,L,s,j,g,maxcd,h,m; 
f:=expand(polmonico); 
L:=[]; 
s:=1; 
while f&lt;&gt;1 do 
j:=1; 
maxcd:=mcdzpx(f,expand(diff(f,x)),p,x); 
g:=quo(f, maxcd, x) mod p; 
while g &lt;&gt; 1 do 
f:=quo(f,g,x) mod p; 
h:=mcdzpx(f,expand(g),p,x); 
m:=quo(g, h, x) mod p; 
if m &lt;&gt; 1 then 
L:=[op(L), [m, j*s]] 
fi; 
g:=h; 
j:=j+1 
od; 
if f &lt;&gt; 1 then 
f:=CreaElPolinomiog(p, f, PasoPolinomioAVector(p, f)); 
s:=p*s 
fi; 
od; 
L;
end:</Text-field>
</Input>
</Group>
