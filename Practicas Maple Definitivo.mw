<Group labelreference="L11" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">restart;
<Font foreground="[51,102,255]">#Algoritmo de euclides</Font>
mcdfpx:=proc(a::polynom,b::polynom,p::integer,x)
local dividendo,divisor,resto,resultado;
dividendo:=a mod p;
sort(dividendo);
divisor:=b mod p;
sort(divisor);
while degree(divisor,x)&gt;=0 do
resto:=rem(dividendo,divisor,x) mod p;
dividendo,divisor:=divisor,resto;
od;
resultado:=factor((dividendo*inversofp(lcoeff(dividendo),p)) mod p);
end:

<Font foreground="[51,102,255]" encoding="UTF-8">#Copio la pr\303\241ctica 4.1</Font>
inversofp:=proc(a::integer,p::integer)
local b,n,i,resultado;
if gcd(p,a)&lt;&gt;1 then 
print(&quot;No hay inverso&quot;);
else
b:=a mod p;
i:=b;
for n from 1 to p do
if i&lt;&gt;1 then
i:=b*n mod p;
resultado:=n;
elif b=1 then
resultado:=1;
fi;
od;
return resultado;
fi;
end:

<Font foreground="[51,102,255]">#Algoritmo de euclides extendido</Font>
euclidesextfpx:=proc(a::polynom,b::polynom,p::integer,x)
local f,g,cociente,coeff,coefg,tf,tg,sf,sg,aux1,aux2,aux3;
if degree (a mod p,x)&lt;degree(b mod p,x) then
f,g:=b mod p, a mod p;
else f,g:=a mod p, b mod p;
fi;
coeff,coefg:=lcoeff(f) mod p, lcoeff(g) mod p;
f,g:=f*inversofp(coeff,p) mod p, g*inversofp(coefg,p) mod p;
tf,tg:=inversofp(coeff,p) mod p,0;
sf,sg:=0,inversofp(coefg,p) mod p;
cociente:=quo(f,g,x);
while degree(cociente,x) mod p &lt;&gt;0 do
aux1,aux2,aux3:=f-cociente*g mod p, tf-cociente*tg,sf-cociente*tf mod p;
coeff:=lcoeff(simplify(aux mod p) mod p) mod p;
f,g:=g,aux1*tf mod p;
tf,tg:=tg,aux2*inversofp*tf mod p;
sf,sg:=sg,aux3*tf mod p;
od;
print(simplify(g),`=(`,a,`)*(`,simplify(tg),`)+(`,b,`)*(`,simplify(sg),` )`);
end:</Text-field>
</Input>
</Group>
