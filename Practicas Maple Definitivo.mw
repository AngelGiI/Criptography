<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">restart;
<Font foreground="[51,102,255]" encoding="UTF-8">#Copiamos la pr\303\241ctica 4.2</Font>
inversofq:=proc(f::polynom,p::integer,a) <Font foreground="[51,102,255]">#donde f es un polinomio irreducible en Fp
</Font>local b,q;
if gcd(p,a)&lt;&gt;1 then 
return false;
else
q:=p^degree(f);
b:=a^(q-2);
return b;
fi;
end:

mcdfqx:=proc(a::polynom,b::polynom,f::polynom,p::prime,x) <Font foreground="[51,102,255]">#donde f es un polinomio irreducible en Fp</Font>
local dividendo,divisor,resto0,resto,resultado;
dividendo:=rem(a,f,x);
sort(dividendo);
divisor:=rem(b,f,x);
sort(divisor);
while degree(divisor,x)&gt;=0 do
resto0:=rem(dividendo,divisor,x);
resto:=rem(resto0,f,x);
dividendo,divisor:=divisor,resto;
od;
if inversofq(f,p,lcoeff(dividendo))=false then
print(&quot;no hay mcd&quot;);
else
resultado:=factor(((dividendo*inversofq(f,p,lcoeff(dividendo))))/f);
fi;
end:</Text-field>
</Input>
