while e&lt;&gt;0 and m&lt;2*B*gamma do<Font foreground="[51,102,255]">#4.Iteramos hasta obtener la factorizacion en Z[x] o hasta llegar a la cota
</Font>c:=quo(e,m,x);
SIGMA:=mods(s*c,p);
TAO:=mods(t*c,p);
q:=quo(simplify(SIGMA),simplify(w1),x);
r:=rem(simplify(SIGMA),simplify(w1),x);
sigma:=simplify(mods(r,p));<Font foreground="[51,102,255]">
</Font>tao:=simplify(mods(TAO+q*u1,p));
u:=simplify(u+tao*m);
w:=simplify(w+sigma*m);
e:=simplify(a-u*w);
m:=m*p;
od;
if e=0 then<Font foreground="[51,102,255]">#5.Obtenemos la factorizacion
</Font>delta:=cont(u);
u:=u/delta;
