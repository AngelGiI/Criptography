<Section collapsed="true" isCollapsible="true" drawButton="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title><Text-field style="Heading 2" layout="Heading 2">1.2. R=Q[x] </Text-field></Title>
<Group labelreference="L22" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">restart;
<Font foreground="[51,102,255]">#Algoritmo de euclides</Font>
mcdqx:=proc(a::polynom,b::polynom,x)
local dividendo,divisor,resto,resultado;
dividendo:=a;
sort(dividendo);
divisor:=b;
sort(divisor);
while degree(divisor,x)&gt;=0 do
resto:=rem(dividendo,divisor,x);
dividendo,divisor:=divisor,resto;
od;
resultado:=factor(dividendo/lcoeff(dividendo));
end:

<Font foreground="[51,102,255]">#Algoritmo de euclides extendido</Font>
euclidesextqx:=proc(f::polynom,g::polynom,x) 
local mcd,resto,coeff,coefg,t,s,cociente; 
if degree(f,x)&lt;degree(g,x) then 
mcd,resto:=g,f;
else mcd,resto:=f,g;
fi; 
coefg,coeff:=0,1; 
t,s:=1,0; 
while resto&lt;&gt;0 do 
mcd,resto,cociente:=resto,rem(mcd,resto,x),quo(mcd,resto,x); 
coeff,s:=s,coeff-cociente*s; 
coefg,t:=t,coefg-cociente*t; 
od; 
coeff:=expand(coeff); 
coefg:=expand(coefg); 
if degree(mcd,x)=0 then 
print(1,`=(`,f,`)*(`,coeff/mcd,`)+(`,g,`)*(`,coefg/mcd,`) `); 
elif mcd mod(coeff(mcd,x,degree(mcd,x)))=0 then 
print(primpart(mcd,x),`=(`,f,`)*(`,coeff/coeff(mcd,x,degree(mcd,x)),`)+(`,g,`)*(`,coefg/coeff(mcd,x,degree(mcd,x)),`) `); 
else
print(mcd,`=(`,f,`)*(`,coeff,`)+(`,g,`)*(`,coefg,`) `); 
fi;
end:</Text-field>
</Input>
</Group>
