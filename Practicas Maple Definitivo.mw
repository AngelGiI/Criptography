Mkalfa := proc (p, f, k, alfa) 
local M, j, expte, d; 
if p = 2 then 
M := alfa; 
for j to k-1 do 
expte := 2^j; 
M := M+alfa^expte; 
M :=rem(M, f, x) mod p; 
