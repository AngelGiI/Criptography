i:=1;
while k&lt;&gt;rg do
i:=i+1;
s:=0;
sigue:=true;
while s&lt;=(p-1) and sigue do
g:=mods(Gcd(W[i-s],c),p);
if g&lt;&gt;1 then
factors[k]:=g;
k:=k+1;
if k=rg then
sigue:=false;
fi;
mods(Divide(c,g,'q'),p); <Font foreground="[51,102,255]">#mira si g divide a u y en caso afirmativo hace q tal que u=g*q
</Font>c:=q;
fi;
s:=s+1;
od;
od;
factors[k]:=c;
fact:=1;
for i from 1 to k do
fact:=fact*factors[i];
od;
return factor(fact);
