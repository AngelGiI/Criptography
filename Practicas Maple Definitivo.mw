<Font foreground="[51,102,255]">#Algoritmo principal</Font>
FactorizacionIgualGrado := proc (p::integer,pol::polynom,k::integer) 
local f,H,l,H1,i,r,aleat,alfa,repMkalfa,h,d; 
f:=pol; 
H:=[f]; 
l:=degree(f, x); 
r:=l/k; 
while nops(H) &lt; r do 
H1:=[]; 
for i to nops(H) do 
h:=H[i]; 
alfa:=Generate(polynom(integer, x, degree=(degree(h)-1))) mod p; 
repMkalfa:=rem(Mkalfa(p, f, k, alfa), f, x) mod p; 
d:=mcdzpx[X](repMkalfa,(expand(h) mod p),p,x); 
if d=1 or d=h then 
H1:=[op(H1),h];
else 
H1:=[op(H1),d]; 
H1:=[op(H1),expand(quo(h, d, x)) mod p]; 
fi; 
od; 
H:=H1; 
od; 
H;
end:</Text-field>
