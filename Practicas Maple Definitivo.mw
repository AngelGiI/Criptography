<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">restart;<Font foreground="[51,102,255]">
<Font encoding="UTF-8">#Copiamos la pr\303\241ctica 1.5 con alguna modificaci\303\263n</Font></Font>
inverso:=proc(a::integer,p::integer)
local b,n,i,resultado;
if gcd(p,a)&lt;&gt;1 then 
print(&quot;No hay inverso&quot;);
else
b:=a mod p;
i:=b;
for n from 1 to p do
if i&lt;&gt;1 then
i:=b*n mod p;
resultado:=n;
elif b=1 then
resultado:=1;
fi;
od;
return resultado;
fi;
end:

<Font foreground="[51,102,255]" encoding="UTF-8">#Copiamos la pr\303\241ctica 4</Font>
mcdzpx:=proc(a::polynom,b::polynom,p::integer,x)
local dividendo,divisor,resto,resultado;
dividendo:=a mod p;
sort(dividendo);
divisor:=b mod p;
sort(divisor);
while degree(divisor,x)&gt;=0 do
resto:=rem(dividendo,divisor,x) mod p;
dividendo,divisor:=divisor,resto;
od;
resultado:=factor((dividendo*inverso(lcoeff(dividendo),p)) mod p);
end:

<Font foreground="[51,102,255]">#Algoritmo principal</Font>
FactorizacionDistintoGrado:=proc(p::prime,f::polynom)<Font foreground="[51,102,255]">#f tiene que ser un polinomio libre de cuadrados</Font>
local g,factores,r,restos,n,h,i; 
g:=f; 
factores:=[]; 
r:=rem(x, g, x) mod p; 
n:=0; 
while degree(g, x) &lt;&gt; 0 do 
restos:=1; 
for i to p do 
restos:=restos*r;
od; 
r:=rem(restos, g, x) mod p; 
n:=n+1; 
h:=mcdzpx(g,(expand(r-x) mod p),p,x) mod p; 
if h &lt;&gt; 1 then 
factores:=[op(factores),[h,n]]; 
g:=quo(g,h,x) mod p; 
r:=rem(r,g,x) mod p; 
fi; 
od; 
factores; 
end:</Text-field>
</Input>
</Group>
