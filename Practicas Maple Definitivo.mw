<Group labelreference="L2453" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">restart;<Font foreground="[51,102,255]">
<Font encoding="UTF-8">#Copiamos la pr\303\241ctica 4.1</Font></Font>
inversofp:=proc(a::integer,p::integer)
local b,n,i,resultado;
if gcd(p,a)&lt;&gt;1 then 
print(&quot;No hay inverso&quot;);
else
b:=a mod p;
i:=b;
for n from 1 to p do
if i&lt;&gt;1 then
