<Group labelreference="L2488" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">mcdzpx:=proc(a::polynom,b::polynom,x,p::integer)
local dividendo,divisor,resto,resultado;
dividendo:=a mod p;
sort(dividendo);
divisor:=b mod p;
sort(divisor);
while degree(divisor,x)&gt;=0 do
resto:=rem(dividendo,divisor,x) mod p;
dividendo,divisor:=divisor,resto;
od;
resultado:=factor((dividendo*inverso(lcoeff(dividendo),p)) mod p);
end:

inverso:=proc(a::integer,p::integer)
local b,n,i,resultado;
if gcd(p,a)&lt;&gt;1 then 
print(&quot;No hay inverso&quot;);
else
b:=a mod p;
i:=b;
for n from 1 to p do
if i&lt;&gt;1 then
i:=b*n mod p;
resultado:=n;
elif b=1 then
resultado:=1;
fi;
od;
return resultado;
fi;
end:

<Font foreground="[51,102,255]">#Test de irreducibilidad</Font>
testirreducibilidadfpx:=proc(f::polynom,x,p::integer) #donde x es la variable con la que trabajamos
local i, m, d, r, esirreducible; 
m:=x; 
d:=degree(f); 
esirreducible:=true;
i:=1;
while i&lt;floor(d/2) and esirreducible do 
m:=rem(m^p, f, x) mod p; 
r:=mcdzpx(f, m-x, x, p);
if r &lt;&gt; 1 then 
esirreducible:=false;
fi;
i:=i+1;
od; 
return esirreducible;
end: 

<Font foreground="[51,102,255]">#algoritmo que nos da un cuerpo Fq

</Font>CreaCuerpo:=proc(f::polynom,p::prime)
local esirred,firred;
esirred:=testirreducibilidadfpx(f,x,p);
if esirred=true then
firred:=f;
print(&quot;es cuerpo&quot;);
else
print(&quot;no es cuerpo&quot;);
fi;
end:
